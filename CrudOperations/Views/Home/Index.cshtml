@{
    bool isTrue = false;
    int myVar = 1;
    var myList = new List<int> { 1, 2, 3, 4 };
}
@if (isTrue)
{
    <h1>Truestatement</h1>
}
else
{
    <h1>Falsestatement</h1>
}
<div>
    @(myVar == 1 ? "True":"False")
</div>
<div>
    @for(int i = 0; i<5; i++)
            {
                <p>for loop @i index </p>
            }
</div>
<div>
    @foreach(var obj in myList)
            {
                <p>my value is =   @obj</p>
            }
</div>

<div>
    @helper Mylisthelper(string[] str)
    {
       <ul>
           @foreach (var item in str)
           {
            <li> @item </li>
           }
       </ul> 
    }
</div>
<div>
        <label>Exmaple 1- Name list</label>
        <div>
                @Mylisthelper(new string[] {"n","k","m","s"})
        </div>
 </div>
<div>
        <div>
            @Mylisthelper(new string[] { "1","2","3"})
        </div>
</div>
<div>
    @Html.ActionLink("Click me","about",new { name = "nidhi"})
</div>

<div>
    @{
        string n = "nidhi sahu";
    }
    @Html.TextBox("txtbox1", n, new { id = "customId", @class = "myCustomClass", style = "width:200px; border-color:blue;" })<br />
    @Html.TextArea("txtarea")<br />
    @Html.CheckBox("chk1", true, new { })<br />
    @Html.Password("pass")
    @using (Html.BeginForm()) { } @*for starting forms*@

    @*for radio button 
        Male
    @Html.RadioButtonFor(model => model.Gender, "Male")
    Female
    @Html.RadioButtonFor(model => model.Gender, "Female")*@

<div>
    @using (Html.BeginForm("myForm"))
    {
        @Html.TextBoxFor(x => x.id)<br />
        @Html.TextBoxFor(x => x.name)<br />
        @Html.TextBoxFor(x => x.address)<br/>
        <input type="submit" value="Submit" />
    }
</div>
</div>

@* templated html helpers *@
<div>
    @Html.Display("Name")
    @Html.DisplayFor(x=>x.name)
    @Html.Display("Address")
    @Html.DisplayText("Name")
    @*@Html.DisplayTextFor("Address")*@
    @Html.Display("Name",new {style="border-color:red;"} )
    @Html.DisplayForModel()
</div>

<div>
    id-  @Html.Editor("id")  @*editor *@

    Name - @Html.EditorFor(x=>x.name)

    date - @Html.EditorFor(x=>x.date)
</div>
